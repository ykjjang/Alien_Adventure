using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class shark_move : MonoBehaviour
{
    public float Wall_speed = -10;
    void Start()
    {
        Destroy(gameObject, 10f);
    }


    void Update()
    {
        transform.Translate(Wall_speed * Time.deltaTime, 0, 0);
    }
}
